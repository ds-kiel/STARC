    <width>500</width>
    <z>4</z>
    <height>200</height>
    <location_x>30</location_x>
    <location_y>495</location_y>
  </plugin>
  <plugin>
    org.contikios.cooja.plugins.Vanet
    <width>150</width>
    <z>3</z>
    <height>300</height>
    <location_x>31</location_x>
    <location_y>31</location_y>
  </plugin>
  <plugin>
    org.contikios.cooja.plugins.LogListener
    <plugin_config>
      <filter>received</filter>
      <formatted_time />
      <coloring />
